<?xml version="1.0" encoding="utf-8" ?>
<config xmlns="urn:schema-unity-config"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="urn:schema-unity-config ../Plugins/OneBuild/config.xsd">

  <Type type="UnityEditor.Build.OneBuild.EditorOneBuild">
    <VersionCode>1</VersionCode>
  </Type>

  <Type type="UnityEditor.PlayerSettings.Android">
    <SetScriptingBackend>
      <BuildTargetGroup>{$Build:BuildTargetGroup}</BuildTargetGroup>
      <ScriptingImplementation>IL2CPP</ScriptingImplementation>
    </SetScriptingBackend>
  </Type>

  <Type type="UnityEditor.PlayerSettings.iOS">
    <TargetDevice>iPhoneAndiPad</TargetDevice>
    <SdkVersion>DeviceSDK</SdkVersion>
    <AppleDeveloperTeamID>{AppleDeveloperTeamID}</AppleDeveloperTeamID>
    <AppleEnableAutomaticSigning>true</AppleEnableAutomaticSigning>
  </Type>

</config>