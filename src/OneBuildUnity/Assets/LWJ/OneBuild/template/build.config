<?xml version="1.0" encoding="utf-8" ?>
<config xmlns="urn:schema-config"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="urn:schema-config ../LWJ/OneBuild/config.xsd">

  <productName>{ProductName}</productName>
  <companyName>{CompanyName}</companyName>
  <applicationIdentifier >{com.xxx.xxx}</applicationIdentifier>
  <version>1.0.0</version>
  <output.dir>Build</output.dir>
  <scriptingBackend>
    <BuildTargetGroup>{$BuildTargetGroup}</BuildTargetGroup>
    <ScriptingImplementation>IL2CPP</ScriptingImplementation>
  </scriptingBackend>
  <il2CppCompilerConfiguration>
    <BuildTargetGroup>{$BuildTargetGroup}</BuildTargetGroup>
    <Il2CppCompilerConfiguration>Release</Il2CppCompilerConfiguration>
  </il2CppCompilerConfiguration>
  <Advertisements.AdvertisementSettings.testMode>true</Advertisements.AdvertisementSettings.testMode>
  <Analytics.AnalyticsSettings.testMode>true</Analytics.AnalyticsSettings.testMode>
  <architecture>
    <BuildTargetGroup>{$BuildTargetGroup}</BuildTargetGroup>
    <architecture>2</architecture>
  </architecture>
  <output.dir>Build/{$BuildTargetGroup}/Release</output.dir>
</config>